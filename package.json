{
  "name": "next-parade",
  "version": "0.0.18",
  "files": [
    "props.js",
    "lib",
    "dist",
    "esm"
  ],
  "main": "dist/index",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "prepare": "yarn bundle",
    "bundle-cjs": "rimraf ./dist && tsc --project tsconfig.lib.json",
    "bundle-esm": "rimraf ./esm && tsc --project tsconfig.lib.json --outDir esm --module es2015",
    "bundle": "yarn bundle-cjs && yarn bundle-esm",
    "watch": "tsc --project tsconfig.lib.json --outDir esm --module es2015 --watch",
    "dev": "next dev",
    "build": "next build",
    "start": "next start"
  },
  "peerDependencies": {
    "next": ">=10",
    "react": ">=16",
    "react-dom": ">=16"
  },
  "dependencies": {
    "fs-extra": "10.0.0",
    "lodash": "4.17.21",
    "react-docgen": "5.4.0",
    "react-docgen-actual-name-handler": "2.1.0",
    "react-docgen-typescript": "1.22.0",
    "recast": "0.20.4"
  },
  "devDependencies": {
    "@types/fs-extra": "9.0.11",
    "@types/lodash": "4.14.169",
    "@types/next": "9.0.0",
    "@types/node": "14.14.45",
    "@types/react": "17.0.5",
