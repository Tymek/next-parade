{
  "name": "next-parade",
  "version": "0.1.1",
  "files": [
    "props.js",
    "lib",
    "dist",
    "esm"
  ],
  "main": "dist/index",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "prepare": "yarn bundle",
    "bundle-cjs": "rimraf ./dist && tsc --project tsconfig.lib.json",
    "bundle-esm": "rimraf ./esm && tsc --project tsconfig.lib.json --outDir esm --module es2015",
    "bundle": "yarn bundle-cjs && yarn bundle-esm",
    "watch": "tsc --project tsconfig.lib.json --outDir esm --module es2015 --watch",
    "dev": "next dev",
    "build": "next build",
    "start": "next start"
  },
  "peerDependencies": {
    "next": ">=12",
    "react": ">=17",
    "react-dom": ">=17"
  },
  "dependencies": {
    "fs-extra": "10.0.0",
    "globby": "12.0.2",
    "lodash": "4.17.21",
    "react-docgen": "5.4.0",
    "react-docgen-actual-name-handler": "2.1.0",
    "react-docgen-typescript": "2.1.1",
    "recast": "0.20.5"
  },
  "devDependencies": {
    "@types/fs-extra": "9.0.13",
    "@types/lodash": "4.14.176",
    "@types/next": "9.0.0",
    "@types/node": "16.11.6",
    "@types/react": "17.0.34",
    "@types/react-dom": "17.0.11",
    "@types/webpack-env": "1.16.3",
    "next": "12.0.2",
